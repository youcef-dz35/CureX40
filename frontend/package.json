{
  "name": "curex40-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "CureX40 Smart Pharmacy Platform - Next-generation digitized healthcare ecosystem with AI, IoT, and automation",
  "type": "module",
  "author": "CureX40 Team",
  "keywords": [
    "smart-pharmacy",
    "healthcare",
    "ai-powered",
    "iot-integration",
    "digital-health",
    "prescription-management",
    "insurance-automation",
    "government-dashboard"
  ],
  "homepage": "https://curex40.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/curex40/platform-frontend.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@types/node": "^24.5.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.19",
    "i18next": "^25.5.2",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.9.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
